@model CloudTrixApp.Models.ProjectStatus

@{
    ViewBag.Title = "Details";
}
<div class="content-wrapper">
    <section class="content-header">
        <h1>

            <small></small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li><a href="#">Company</a></li>
            <li class="active">Project Status Details  </li>
        </ol>
    </section>
    <div class="wraper container-fluid ">
        <div class="row">
            <div class="col col-md-11">
                <div class="panel panel-primary">
                    <div class="panel-heading">
                        <h3 class="panel-title">Project Status Details  </h3>
                    </div>
                    <div class="panel-body">
                        <hr />
                        @Html.HiddenFor(model => model.ProjectStatusID)

                        <div class="container-fluid">
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label for="ProjectStatusName"> Project Status Name : </label>
                                    @Html.TextBoxFor(model => model.ProjectStatusName, null, new { @class = "form-control ", @disabled="off",@placeholder = "Enter Project Status Name " })
                                    @Html.ValidationMessageFor(model => model.ProjectStatusName)
                                </div>
                            </div>
                        </div>
                    </div>
                    <p>
                       
                    </p>
                    <div class="container-fluid">
                        <div><hr size=2><br /></div>
                        <div class="col-sm-4 col-lg-offset-4">
                            <div class="form-group">
                                &nbsp;&nbsp;
                                @Html.ActionLink("Edit", "Edit", new{ProjectStatusID = Model.ProjectStatusID}) |
                                @Html.ActionLink("Back to List", "Index", null, null, new { @class = "btn btn-primary" })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>



