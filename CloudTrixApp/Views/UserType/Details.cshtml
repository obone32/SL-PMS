@model CloudTrixApp.Models.UserType

@{
    ViewBag.Title = "Details";
}

<div class="content-wrapper">
    <section class="content-header">
        <h1>

            <small></small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li><a href="#">Master</a></li>
            <li class="active">User Type Details  </li>
        </ol>
    </section>
    <div class="wraper container-fluid ">
        <div class="row">
            <div class="col col-md-11">
                <div class="panel panel-primary">
                    <div class="panel-heading">
                        <h3 class="panel-title">User Type Details  </h3>
                    </div>
                    <div class="panel-body">
                        <hr />
                        @Html.HiddenFor(model => model.UserTypeID)

                        <div class="container-fluid">
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label for="ProjectStatusName"> User Type Name : </label>
                                    @Html.TextBoxFor(model => model.UserTypeName, null, new { @class = "form-control ", @disabled = "off", @placeholder = "Enter User Type Name " })
                                    @Html.ValidationMessageFor(model => model.UserTypeName)
                                </div>
                            </div>
                        </div>
                    </div>
                    <p>

                    </p>
                    <div class="container-fluid">
                        <div><hr size=2><br /></div>
                        <div class="col-sm-4 col-lg-offset-4">
                            <div class="form-group">
                                &nbsp;&nbsp;
                                @Html.ActionLink("Edit", "Edit", new { UserTypeID = Model.UserTypeID }) |
                                @Html.ActionLink("Back to List", "Index", null, null, new { @class = "btn btn-primary" })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
 
